class Highway is subclass of Spot

types
	public Type = <ENTRANCE> | <EXIT>;
	
instance variables
	public type: Type;
	
operations
	public Highway : nat1 * nat1 * Type ==> Highway
	 Highway(lat, long, t) == (
	 	local := mk_Local(lat, long);
	 	type := t;
		return self;
	);
	
	public getType : () ==> Type 
	getType() == (
		return type;
	);
	
end Highway