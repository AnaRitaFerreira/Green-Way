class Client
	
instance variables
	public name : seq1 of char;
	public payment_card : int; 
	
operations
	--cliente tem informação do nome e do numero de cartao para pagamento
	public Client : seq1 of char * int ==> Client
	 Client(nome, pay_card) == (
		name := nome;
		payment_card := pay_card;
		return self;
	);
	
	
	
end Client