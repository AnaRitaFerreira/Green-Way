class Spot
types

	--é a classe dos postos em que o cliente pode passar
	--guarda a informação da posição 
	public Local :: 	latitude: nat1
										longitude: nat1;
												
instance variables
	public local: Local;
	
operations
	public Spot : nat1 * nat1 ==> Spot
	 Spot(lat, long) == (
	 	local := mk_Local(lat, long);
		return self;
	);
	

end Spot