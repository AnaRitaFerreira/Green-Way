class Time
instance variables

public timer: nat1;
public month: nat1;
public year: nat1;

operations

public static Diff : Time * Time ==> real 
Diff (time1,time2) == (
	 return time1.timer - time2.timer;
);

public Time : nat1 ==> Time
	 Time(t) == (
	 	month := 1;
	 	year := 2015;
	 	timer := t;
		return self;
	);
	
functions

	public compareTimes : nat1 * nat1 -> int
		compareTimes(time1, time2) == (
		time1- time2
	);
	
end Time