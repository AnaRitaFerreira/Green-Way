class Time
instance variables

public month: nat1;
public year: nat1;

operations

public Time : () ==> Time
	 Time() == (
	 	month := 1;
	 	year := 2015;
		return self;
	);
	
public setMonth : nat1 ==> ()
setMonth (m) == (
	month := m;
);

public setYear : nat1 ==> ()
setYear (y) == (
	year := y;
);
	
end Time